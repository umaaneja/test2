{
  "name": "your-vendor/paragraphs",
  "description": "Reusable paragraph system for Laravel",
  "type": "library",
  "autoload": {
    "psr-4": {
      "YourVendor\\Paragraphs\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "YourVendor\\Paragraphs\\ParagraphsServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^8.0",
    "illuminate/support": "^8.0|^9.0|^10.0"
  }
}
